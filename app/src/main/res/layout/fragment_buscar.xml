<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home"
    tools:context=".ui.home.BuscarFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="742dp"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/imageButtonHomeS"
            android:layout_width="37dp"
            android:layout_height="37dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="460dp"
            android:src="@drawable/home1" />

        <ImageButton
            android:id="@+id/imageButtonSearcheS"
            android:layout_width="35dp"
            android:layout_height="37dp"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="460dp"
            android:src="@drawable/searche1" />

        <ImageButton
            android:id="@+id/imageButtonProfileS"
            android:layout_width="35dp"
            android:layout_height="37dp"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="460dp"
            android:src="@drawable/pessoa" />

        <ImageButton
            android:id="@+id/imageButtonOrcS"
            android:layout_width="35dp"
            android:layout_height="37dp"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="460dp"
            android:src="@drawable/orc" />

        <ImageButton
            android:id="@+id/imageButtonConfigS"
            android:layout_width="35dp"
            android:layout_height="37dp"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="460dp"
            android:src="@drawable/config" />
    </LinearLayout>
</FrameLayout>